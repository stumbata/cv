---
import Card from "../Card.astro";
const my_info: any = Object.values(import.meta.glob('@/my_info.json', { eager: true }))[0];
---

<Card title="COURSES">
    {my_info.courses.map((course_title: any) => (
      <div class="course-title">{course_title}</div>
      <hr class="dashed-line" />
    ))}
</Card>
